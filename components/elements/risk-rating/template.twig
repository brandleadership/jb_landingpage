<div data-bsi-element="risk-rating" class="risk-bar risk-level-5">
    <section class="template-section-spacer--media-components">
        <div class="row center-xs">
            <div class="col-md-8 col-sm-12 col-xs-12">
                <div class="media-image">
                    <div class="media-image__wrap">
                        {% include '../element-heading/template.twig' with {
                            isNoElement: true
                        } %}
                        <div class="risk-rating-box">
                            <div class="risk-rating__rating">
                                <div class="risk-rating-element highlight-first">
                                    <p>
                                        1
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-second">
                                    <p>
                                        2
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-third">
                                    <p>
                                        3
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-fourth">
                                    <p>
                                        4
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-fifth">
                                    <p>
                                        5
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-sixth">
                                    <p>
                                        6
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-seventh">
                                    <p>
                                        7
                                    </p>
                                </div>
                            </div>
                            <div class="risk-rating__legend">
                                <div class="line-container">
                                    <span class="line arrow-right"></span>
                                </div>
                                <div class="legend-box">
                                    <p>
                                        Lower Risk
                                    </p>
                                    <p>
                                        Higher Risk
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="text-block col-xs-12 module-content-gallery__content element-description">
                            <div class="text-small module-content-gallery__content-caption">
                                <p>
                                    <b class="risk-level"></b>
                                    <span class="conclusion"></span>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
