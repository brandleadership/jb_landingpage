<div data-bsi-element="risk-rating-website" class="risk-bar risk-level-5">
    <section class="template-section-spacer--media-components">
        <div class="row center-xs">
            <div class="col-md-8 col-sm-12 col-xs-12">
                <div class="media-image">
                    <div class="media-image__wrap">
                        {% include '../element-heading/template.twig' with {
                            isNoElement: true
                        } %}
                        <div class="risk-rating-box">
                            <div class="risk-rating__rating">
                                <div class="risk-rating-element highlight-first">
                                    <p>
                                        1
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-second">
                                    <p>
                                        2
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-third">
                                    <p>
                                        3
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-fourth">
                                    <p>
                                        4
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-fifth">
                                    <p>
                                        5
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-sixth">
                                    <p>
                                        6
                                    </p>
                                </div>
                                <div class="risk-rating-element highlight-seventh">
                                    <p>
                                        7
                                    </p>
                                </div>
                            </div>
                            <div class="risk-rating__legend">
                                <div class="line-container">
                                    <span class="line arrow-right"></span>
                                </div>
                                <div class="legend-box">
                                    <p>
                                         {% verbatim %}{{ bsi.nls 'lower_risk' }}{% endverbatim %}
                                    </p>
                                    <p>
                                        {% verbatim %}{{ bsi.nls 'higher_risk' }}{% endverbatim %}
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="text-block col-xs-12 module-content-gallery__content element-description">
                            <div class="text-small module-content-gallery__content-caption">
                                <p>
                                 
                                    <b class="risk-level"></b>
                                    <span class="conclusion"></span>
                                    <style type="text/css">
                                        .highlight-first .risk-level::before {
                                        content: "{% verbatim %}{{ bsi.nls 'risk_level_1' }}{% endverbatim %}"
                                        } .highlight-second .risk-level::before
                                        { content: "{% verbatim %}{{ bsi.nls 'risk_level_2'
                                        }}{% endverbatim %}" } .highlight-third
                                        .risk-level::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_level_3'
                                        }}{% endverbatim %}" } .highlight-fourth
                                        .risk-level::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_level_4'
                                        }}{% endverbatim %}" } .highlight-fifth
                                        .risk-level::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_level_5'
                                        }}{% endverbatim %}" } .highlight-sixth
                                        .risk-level::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_level_6'
                                        }}{% endverbatim %}" } .highlight-seventh
                                        .risk-level::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_level_7'
                                        }}{% endverbatim %}" } .highlight-first
                                        .conclusion::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_rating_1'
                                        }}{% endverbatim %}" } .highlight-second
                                        .conclusion::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_rating_2'
                                        }}{% endverbatim %}" } .highlight-third
                                        .conclusion::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_rating_3'
                                        }}{% endverbatim %}" } .highlight-fourth
                                        .conclusion::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_rating_4'
                                        }}{% endverbatim %}" } .highlight-fifth
                                        .conclusion::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_rating_5'
                                        }}{% endverbatim %}" } .highlight-sixth
                                        .conclusion::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_rating_6'
                                        }}{% endverbatim %}" } .highlight-seventh
                                        .conclusion::before { content: "{% verbatim %}{{
                                            bsi.nls 'risk_rating_7'
                                        }}{% endverbatim %}" }
                                    </style>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
