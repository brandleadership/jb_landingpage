<div data-bsi-element="radio-buttons"
    class="radiobutton-container-wrapper"
    style="padding-bottom: 15px;">
    <div id="option-radio-group"
        data-bsi-element-part="form-radio"
        data-bsi-value-list="1|1
2|2"
        data-bsi-fixed-value-list="">
        <div data-bsi-radio-group="" class="radiobutton-set-container">
            <div data-bsi-radio-item="" class="custom-radiobutton-container">
                <input type="radio"
                    name="option-radio-group"
                    id="option-radio-group-1"
                    class="radio-btn"
                    value="Option-1"
                    spellcheck="true" />
                <label for="option-radio-group-1">1</label>
                <span class="custom-radiobutton-frame"></span>
            </div>
            <div data-bsi-radio-item="" class="custom-radiobutton-container">
                <input type="radio"
                    class="radio-btn"
                    name="option-radio-group"
                    id="option-radio-group-2"
                    value="Option-2"
                    spellcheck="true" />
                <label for="option-radio-group-2">2</label>
                <span class="custom-radiobutton-frame"></span>
            </div>
        </div>
         <div data-bsi-radio-item="" class="custom-radiobutton-container">
                <input type="radio"
                    class="radio-btn"
                    name="option-radio-group"
                    id="option-radio-group-3"
                    value="Option-3"
                    spellcheck="true" />
                <label for="option-radio-group-3">3</label>
                <span class="custom-radiobutton-frame"></span>
            </div>
        </div>
         <div data-bsi-radio-item="" class="custom-radiobutton-container">
                <input type="radio"
                    class="radio-btn"
                    name="option-radio-group"
                    id="option-radio-group-4"
                    value="Option-4"
                    spellcheck="true" />
                <label for="option-radio-group-4">4</label>
                <span class="custom-radiobutton-frame"></span>
            </div>
             <div data-bsi-radio-item="" class="custom-radiobutton-container">
                <input type="radio"
                    class="radio-btn"
                    name="option-radio-group"
                    id="option-radio-group-5"
                    value="Option-5"
                    spellcheck="true" />
                <label for="option-radio-group-5">5</label>
                <span class="custom-radiobutton-frame"></span>
            </div>
        </div>
        <div class="error-message-title isVisible">
            {{ properties.form.title_error }}
        </div>
    </div>
</div>
