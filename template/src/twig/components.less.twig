{% set header = ['header-image'] %}

{% for header_element in header %}
    {% include 'content-elements/header/' ~ header_element ~ '/styles.less' %}
{% endfor %}

{% set related_articles = ['articles-content', 'articles-3-columns'] %}

{% for related_article in related_articles %}
